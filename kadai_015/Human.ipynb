        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(self.name, self.age)\n",
        "\n",
        "# インスタンス化する\n",
        "nagafuchi = Human(\"永渕\", 25)\n",
        "\n",
        "# メソッドにアクセスして実行する\n",
        "nagafuchi.printinfo()"
      ]
